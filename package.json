{
  "name": "novelai-image-sdk",
  "version": "1.2.1",
  "description": "Type-safe TypeScript SDK for NovelAI V4.5 image generation API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:live": "vitest run tests/integration.test.ts",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "novelai",
    "image-generation",
    "diffusion",
    "ai",
    "typescript",
    "sdk"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "nai-gen": "./bin/nai-gen.mjs"
  }
}
