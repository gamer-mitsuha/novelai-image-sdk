{
  "name": "novelai-image-sdk",
  "version": "1.3.0",
  "description": "Type-safe TypeScript SDK for NovelAI V4.5 image generation API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:live": "vitest run tests/integration.test.ts",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "novelai",
    "image-generation",
    "ai",
    "anime",
    "diffusion",
    "typescript",
    "sdk",
    "cli",
    "nai",
    "text-to-image",
    "v4.5",
    "multi-character"
  ],
  "author": "gamer-mitsuha",
  "license": "Apache-2.0",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "nai-gen": "./bin/nai-gen.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamer-mitsuha/novelai-image-sdk.git"
  },
  "homepage": "https://github.com/gamer-mitsuha/novelai-image-sdk#readme",
  "bugs": {
    "url": "https://github.com/gamer-mitsuha/novelai-image-sdk/issues"
  }
}
